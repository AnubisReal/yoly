<script>
	// Cargar datos desde localStorage si existen
	async function loadDataAndUpdate() {
		let data;
		const localData = localStorage.getItem('anubisreal-data');
		
		if (localData) {
			data = JSON.parse(localData);
			console.log('Datos cargados desde localStorage');
		} else {
			// Cargar desde data.json si no hay datos locales
			try {
				const response = await fetch('/data.json');
				data = await response.json();
			} catch (error) {
				console.log('No se pudo cargar data.json, usando datos por defecto');
				return;
			}
		}
		
		// Actualizar las cards de redes sociales
		if (data.socialCards) {
			const socialCards = document.querySelectorAll('.social-card');
			data.socialCards.forEach((cardData, index) => {
				if (socialCards[index]) {
					const card = socialCards[index];
					const username = card.querySelector('.username');
					const stats = card.querySelector('.stats');
					const visitButton = card.querySelector('.visit-button');
					const profilePic = card.querySelector('.profile-pic');
					
					if (username) username.textContent = cardData.username;
					if (stats) {
						const spans = stats.querySelectorAll('span');
						if (spans[0]) spans[0].innerHTML = `<strong>${cardData.followers}</strong> Seguidores`;
						if (spans[1]) spans[1].innerHTML = `<strong>${cardData.following}</strong> Siguiendo`;
					}
					if (visitButton && cardData.url) {
						visitButton.href = cardData.url;
					}
					if (profilePic && cardData.profileImage) {
						profilePic.style.backgroundImage = `url(${cardData.profileImage})`;
						profilePic.style.backgroundSize = 'cover';
						profilePic.style.backgroundPosition = 'center';
					}
				}
			});
		}
		
		// Actualizar Spotify
		if (data.spotify && data.spotify.embedUrl) {
			const spotifyIframe = document.querySelector('.spotify-container iframe');
			if (spotifyIframe) {
				spotifyIframe.src = data.spotify.embedUrl;
			}
		}
		
		// Actualizar videos
		if (data.videos) {
			const videosContainer = document.querySelector('[data-tab="videos"] .videos-container');
			if (videosContainer) {
				videosContainer.innerHTML = data.videos.map(video => `
					<div class="video-item">
						<iframe 
							src="${video.url}" 
							title="YouTube video player" 
							frameborder="0" 
							allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" 
							referrerpolicy="strict-origin-when-cross-origin" 
							allowfullscreen>
						</iframe>
					</div>
				`).join('');
			}
		}
	}
	
	// Cargar datos al iniciar
	loadDataAndUpdate();
	
	// Escuchar cambios en localStorage (para actualizar en tiempo real si se abre en otra pestaña)
	window.addEventListener('storage', (e) => {
		if (e.key === 'anubisreal-data') {
			loadDataAndUpdate();
		}
	});

	document.querySelectorAll('.nav-item').forEach(item => {
		item.addEventListener('click', (e) => {
			e.preventDefault();
			
			// Remover active de todos los nav-items
			document.querySelectorAll('.nav-item').forEach(i => i.classList.remove('active'));
			item.classList.add('active');
			
			// Obtener el texto del nav-item para saber qué pestaña activar
			const tabName = item.textContent.trim().toLowerCase().replace(' ', '-');
			
			// Ocultar todos los tab-content
			document.querySelectorAll('.tab-content').forEach(tab => {
				tab.classList.remove('active');
			});
			
			// Mostrar el tab-content correspondiente
			const activeTab = document.querySelector(`.tab-content[data-tab="${tabName}"]`);
			if (activeTab) {
				activeTab.classList.add('active');
			}
		});
	});

	// Manejo de tarjetas sociales para móvil
	const isTouchDevice = 'ontouchstart' in window || navigator.maxTouchPoints > 0;
	
	if (isTouchDevice) {
		const socialCards = document.querySelectorAll('.social-card');
		
		socialCards.forEach(card => {
			card.addEventListener('click', (e) => {
				// Si el clic es en el botón visitar, dejar que funcione normalmente
				if (e.target.classList.contains('visit-button')) {
					return;
				}
				
				// Prevenir el comportamiento por defecto
				e.preventDefault();
				e.stopPropagation();
				
				// Remover la clase active de todas las demás tarjetas
				socialCards.forEach(c => {
					if (c !== card) {
						c.classList.remove('active');
					}
				});
				
				// Toggle de la clase active en la tarjeta actual
				card.classList.toggle('active');
			});
		});
		
		// Cerrar tarjetas al tocar fuera
		document.addEventListener('click', (e) => {
			if (!e.target.closest('.social-card')) {
				socialCards.forEach(card => card.classList.remove('active'));
			}
		});
	}

	// Animación aleatoria de los personajes
	const magicSphere = document.querySelector('.magic-sphere');
	const dead = document.querySelector('.dead');
	let currentFrame = 0;
	let isAnimating = false;
	let animationTimeout;

	function animateCharacters() {
		if (isAnimating) return;
		isAnimating = true;

		// Mostrar y animar magic sphere
		magicSphere.style.opacity = '1';
		dead.style.opacity = '0';
		
		let sphereFrame = 0;
		const sphereInterval = setInterval(() => {
			magicSphere.style.backgroundPosition = `-${sphereFrame * 128}px 0`;
			sphereFrame++;
			
			if (sphereFrame >= 16) {
				clearInterval(sphereInterval);
				
				// Ocultar sphere y mostrar dead
				magicSphere.style.opacity = '0';
				dead.style.opacity = '1';
				
				let deadFrame = 0;
				const deadInterval = setInterval(() => {
					dead.style.backgroundPosition = `-${deadFrame * 128}px 0`;
					deadFrame++;
					
					if (deadFrame >= 4) {
						clearInterval(deadInterval);
						
						// Ocultar dead después de completar
						setTimeout(() => {
							dead.style.opacity = '0';
							isAnimating = false;
							
							// Programar siguiente aparición aleatoria (5-10 segundos)
							const randomDelay = Math.random() * 5000 + 5000;
							animationTimeout = setTimeout(animateCharacters, randomDelay);
						}, 100);
					}
				}, 150); // 0.15s por frame
			}
		}, 150); // 0.15s por frame
	}

	// Iniciar la primera animación al cargar
	setTimeout(animateCharacters, 500);

</script>

